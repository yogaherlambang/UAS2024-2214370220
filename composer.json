{
    "require": {
        "catfan/medoo": "^1.6",
        "slim/slim": "^3.0",
        "monolog/monolog": "^1.24",
        "slim/php-view": "^2.2",
        "illuminate/database": "^10.43",
        "ramsey/uuid": "^3.8",
        "erusev/parsedown": "^1.7",
        "phpmailer/phpmailer": "^6.0",
        "vlucas/phpdotenv": "^3.4",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Model\\": "src/model/",
            "App\\Controller\\": "src/controllers/",
            "App\\Library\\": "src/libs/",
            "App\\Helper\\": "src/helpers/",
            "App\\Middleware\\": "src/middlewares/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "start": "php -S localhost:4001 -t public",
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php test"
    },

    "config": {
        "process-timeout": 0
    }
}
